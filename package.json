{
  "name": "linter-cwl",
  "main": "index.js",
  "version": "0.3.3",
  "private": true,
  "description": "CWL Linter Package",
  "keywords": [
    "lint",
    "linter-cwl",
    "cwl"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/manabuishii/linter-cwl"
  },
  "configSchema": {
    "executablePath": {
      "type": "string",
      "default": "schema-salad-tool",
      "description": "Full path to the schema-salad-tool"
    },
    "validateYamlPath": {
      "type": "string",
      "default": "CommonWorkflowLanguage.yml",
      "description": "Full path to the validation file (ie. CommonWorkflowLanguage.yml)"
    }
  },
  "bugs": {
    "url": "https://github.com/manabuishii/linter-cwl/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.4.0 <2.0.0"
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {
    "atom-linter": "^10.0.0",
    "atom-package-deps": "^4.5.0"
  }
}
